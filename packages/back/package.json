{
  "name": "@feedback/back",
  "version": "1.0.0",
  "description": "Feedback backend service",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "bench": "sh bench.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc --sourceMap -p ./",
    "start": "NODE_ENV=production yarn prisma:migrate:prod && tsx src/server.ts",
    "start2": "NODE_ENV=production prisma migrate deploy && ts-node src/server.ts",
    "ts:check": "tsc --pretty --noEmit",
    "prettify": "prettier --write ./src",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "tunnel": "ssh -p 213 -o \"UserKnownHostsFile=/dev/null\" -o \"StrictHostKeyChecking no\" -N -R \"*:11211:localhost:3005\" publisher@tunnel2.zbc.su #https://tunnel2.zbc.su/",
    "tunnel:restart": "ssh adminvps  'docker-compose -f /root/containers/tunnel2/docker-compose.yml down; docker-compose -f /root/containers/tunnel2/docker-compose.yml up -d'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/rate-limit": "^7.5.0",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^7.30.0",
    "@sentry/tracing": "^7.30.0",
    "axios": "^1.7.9",
    "dotenv-cli": "^8.0.0",
    "fastify": "^4.5.3",
    "fastify-sentry": "^1.4.0",
    "fastify-zod": "^1.1.0",
    "tsx": "^4.19.4",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.17.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "autocannon": "^7.9.0",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
